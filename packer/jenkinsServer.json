{
    "variables": {
        "aws_access_key": "",
        "aws_secret_key": "",
        "region": ""
    },
    "builders": [{
        "type": "amazon-ebs",
        "access_key": "{{user `aws_access_key`}}",
        "secret_key": "{{user `aws_secret_key`}}",
